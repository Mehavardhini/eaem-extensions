<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:vlt="http://www.day.com/jcr/vault/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:mixinTypes="[vlt:FullCoverage]"
    jcr:primaryType="sling:OrderedFolder"
    sling:resourceType="cq/gui/components/authoring/editorpanel"
    default="{Boolean}true"
    id="Content">
    <header
        jcr:primaryType="nt:unstructured"
        class="editor-GlobalBar js-editor-PanelHeader">
        <items jcr:primaryType="nt:unstructured">
            <headerbar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dam/cfm/admin/migration/components/headerbar"
                class="header-main">
                <items jcr:primaryType="nt:unstructured">
                    <togglesidepanel
                        granite:class="toggle-sidepanel editor-GlobalBar-item js-editor-SidePanel-toggle"
                        granite:title="Toggle Side Panel"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="left"
                        disabled="{Boolean}false"
                        icon="railLeft"
                        iconSize="S"
                        variant="minimal"/>
                    <fragmentElements
                        granite:class="editor-GlobalBar-item"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dam/cfm/admin/components/adminpage/title"
                        align="center"
                        pattern="${fragmentName}"/>
                    <save
                        granite:class="button-apply editor-GlobalBar-item"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="right"
                        hideText="{Boolean}false"
                        text="Save"
                        variant="primary">
                        <granite:rendercondition
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/privilege"
                            path="${requestPathInfo.suffix}/jcr:content"
                            privileges="[jcr:addChildNodes,jcr:modifyProperties,jcr:removeChildNodes]"/>
                    </save>
                    <cancel
                        granite:class="button-cancel editor-GlobalBar-item"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="right"
                        hideText="{Boolean}false"
                        text="Cancel"
                        variant="secondary"/>
                </items>
            </headerbar>
        </items>
    </header>
    <content
        jcr:primaryType="nt:unstructured"
        id="ContentScrollView">
        <items jcr:primaryType="nt:unstructured">
            <contentwrapper
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/contentwrapper"/>
            <editors
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dam/cfm/admin/migration/components/shell/editor">
                <metadataeditor
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dam/cfm/admin/components/metadata/editor"/>
                <form
                    granite:class="align-in-column active"
                    granite:id="eaem-content-fragment-playamable-media"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form"
                    action="${granite:encodeURIPath(requestPathInfo.suffix)}.annotation.pdf"
                    foundationForm="{Boolean}true"
                    method="post"
                    style="vertical">
                    <items jcr:primaryType="nt:unstructured">
                        <tabs
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/tabs"
                            size="L">
                            <items jcr:primaryType="nt:unstructured">
                                <tab1
                                    granite:rel="eaem-assets-contentfragment-form-tab"
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Basic"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    listOrder="0">
                                    <granite:data
                                        jcr:primaryType="nt:unstructured"
                                        tabid="132a0788-7a84-4143-ac2b-3ecbb502d3a1"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <col1
                                            granite:rel="aem-assets-metadata-form-column"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            listOrder="0">
                                            <items jcr:primaryType="nt:unstructured">
                                                <title
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Enter title"
                                                    fieldLabel="Title"
                                                    name="./jcr:content/jcr:title"
                                                    required="true">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        metaType="text"/>
                                                </title>
                                                <description
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="Enter description"
                                                    fieldLabel="Description"
                                                    name="./jcr:content/jcr:description">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        metaType="text"/>
                                                </description>
                                                <create_time
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                    disabled="true"
                                                    displayedFormat="YYYY-MM-DD HH:mm"
                                                    fieldLabel="Created"
                                                    name="./jcr:created"
                                                    type="datetime">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        metaType="datepicker"
                                                        typeHint="Date"/>
                                                </create_time>
                                            </items>
                                            <jcr:content jcr:primaryType="nt:unstructured">
                                                <metadata jcr:primaryType="nt:unstructured"/>
                                            </jcr:content>
                                        </col1>
                                        <col2
                                            granite:rel="aem-assets-metadata-form-column"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            listOrder="1">
                                            <items jcr:primaryType="nt:unstructured">
                                                <tags
                                                    cq:showOnCreate="true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/common/tagspicker"
                                                    allowCreate="true"
                                                    cq-msm-lockable="cq:tags"
                                                    fieldLabel="Tags"
                                                    metaType="tags"
                                                    name="./jcr:content/metadata/cq:tags">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        metaType="tags"/>
                                                </tags>
                                            </items>
                                        </col2>
                                    </items>
                                </tab1>
                            </items>
                        </tabs>
                    </items>
                </form>
                <contentpath
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dam/gui/coral/components/commons/ui/shell/contentpath"
                    path="${requestPathInfo.suffix}"/>
            </editors>
        </items>
    </content>
</jcr:root>
