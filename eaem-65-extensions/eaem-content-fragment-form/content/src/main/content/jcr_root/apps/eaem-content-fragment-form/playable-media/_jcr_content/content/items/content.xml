<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:vlt="http://www.day.com/jcr/vault/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:mixinTypes="[vlt:FullCoverage]"
    jcr:primaryType="sling:OrderedFolder"
    sling:resourceType="cq/gui/components/authoring/editorpanel"
    default="{Boolean}true"
    id="Content">
    <header
        jcr:primaryType="nt:unstructured"
        class="editor-GlobalBar js-editor-PanelHeader">
        <items jcr:primaryType="nt:unstructured">
            <headerbar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dam/cfm/admin/migration/components/headerbar"
                class="header-main">
                <items jcr:primaryType="nt:unstructured">
                    <togglesidepanel
                        granite:class="toggle-sidepanel editor-GlobalBar-item js-editor-SidePanel-toggle"
                        granite:title="Toggle Side Panel"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="left"
                        disabled="{Boolean}false"
                        icon="railLeft"
                        iconSize="S"
                        variant="minimal"/>
                    <fragmentElements
                        granite:class="editor-GlobalBar-item"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dam/cfm/admin/components/adminpage/title"
                        align="center"
                        pattern="${fragmentName}"/>
                    <save
                        granite:class="button-apply editor-GlobalBar-item"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="right"
                        hideText="{Boolean}false"
                        text="Save"
                        variant="primary">
                        <granite:rendercondition
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/privilege"
                            path="${requestPathInfo.suffix}/jcr:content"
                            privileges="[jcr:addChildNodes,jcr:modifyProperties,jcr:removeChildNodes]"/>
                    </save>
                    <cancel
                        granite:class="button-cancel editor-GlobalBar-item"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/button"
                        align="right"
                        hideText="{Boolean}false"
                        text="Cancel"
                        variant="secondary"/>
                </items>
            </headerbar>
        </items>
    </header>
    <content
        jcr:primaryType="nt:unstructured"
        id="ContentScrollView">
        <items jcr:primaryType="nt:unstructured">
            <contentwrapper
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/contentwrapper"/>
            <editors
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dam/cfm/admin/migration/components/shell/editor">
                <metadataeditor
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dam/cfm/admin/components/metadata/editor"/>
                <form
                    granite:class="align-in-column active"
                    granite:id="eaem-content-fragment-playamable-media"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form"
                    action="${granite:encodeURIPath(requestPathInfo.suffix)}.annotation.pdf"
                    foundationForm="{Boolean}true"
                    method="post"
                    style="vertical">
                    <items jcr:primaryType="nt:unstructured">
                        <tabs
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/tabs"
                            maximized="{Boolean}true"
                            size="L">
                            <items jcr:primaryType="nt:unstructured">
                                <assets
                                    granite:rel="eaem-assets-contentfragment-form-tab"
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Assets"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <duration
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Duration"
                                                    fieldLabel="Duration"
                                                    name="./duration"/>
                                                <video
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldLabel="Video">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./products">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <mgidFileURI
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="MGID File URI"
                                                                        fieldLabel="MGID File URI"
                                                                        name="./mgidFileURI"/>
                                                                    <container
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Container (mp4 | fmp4 | bitstream | scc | ttml | webvtt)"
                                                                        fieldLabel="Container"
                                                                        name="./container"/>
                                                                    <language
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Container (mp4 | fmp4 | bitstream | scc | ttml | webvtt)"
                                                                        fieldLabel="Language"
                                                                        name="./language"/>
                                                                    <duration
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Duration"
                                                                        fieldLabel="Duration"
                                                                        name="./videoDuration"/>
                                                                    <captionsInfo
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Captions info (SDSWDSDDDDFGG)"
                                                                        fieldLabel="Captions info"
                                                                        name="./captionsInfo"/>
                                                                    <width
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Width"
                                                                        fieldLabel="Width"
                                                                        name="./width"/>
                                                                    <height
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Height"
                                                                        fieldLabel="Height"
                                                                        name="./height"/>
                                                                    <peakBitRateKbps
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Peak Bitrate (Provided in KB/S)"
                                                                        fieldLabel="Peak Bitrate"
                                                                        name="./peakBitRateKbps"/>
                                                                    <avgBitRateKbps
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Average Bitrate (Provided in KB/S)"
                                                                        fieldLabel="Average Bitrate"
                                                                        name="./avgBitRateKbps"/>
                                                                    <hasBurnedInTitles
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        fieldDescription="Has burned-in titles"
                                                                        fieldLabel="Has burned-in titles"
                                                                        name="./hasBurnedInTitles"
                                                                        text="Has burned-in titles"
                                                                        value="true"/>
                                                                    <frameRate
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Frame rate"
                                                                        fieldLabel="Frame rate"
                                                                        name="./frameRate"/>
                                                                    <videoCodec
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Video Codec (Uses RFC6381)"
                                                                        fieldLabel="Video Codec"
                                                                        name="./videoCodec"/>
                                                                    <numberAudioChannels
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Audio Channels (eg. 4)"
                                                                        fieldLabel="Audio Channels"
                                                                        name="./numberAudioChannels"/>
                                                                    <isAudioOverDubbed
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        fieldDescription="Is Audio Over-Dubbed"
                                                                        fieldLabel="Is Audio Over-Dubbed"
                                                                        name="./isAudioOverDubbed"
                                                                        text="Is Audio Over-Dubbed"
                                                                        value="true"/>
                                                                    <audioVideoCodec
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Audio/Video Codec (Uses RFC6381)"
                                                                        fieldLabel="Audio/Video Codec"
                                                                        name="./audioVideoCodec"/>
                                                                    <audioCodec
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Audio Codec"
                                                                        fieldLabel="Audio Codec"
                                                                        name="./audioCodec"/>
                                                                </items>
                                                            </column>
                                                        </items>
                                                    </field>
                                                </video>
                                            </items>
                                        </column>
                                    </items>
                                </assets>
                                <identifiers
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Identifiers"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <section1
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Settings"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <alias
                                                            cq:showOnCreate="{Boolean}true"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Alias"
                                                            name="./sling:alias"
                                                            renderReadOnly="{Boolean}true">
                                                            <granite:data
                                                                jcr:primaryType="nt:unstructured"
                                                                cq-msm-lockable="sling:alias"/>
                                                        </alias>
                                                    </items>
                                                </section1>
                                            </items>
                                        </column>
                                    </items>
                                </identifiers>
                                <cues
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Cues"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <section1
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Settings"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <alias
                                                            cq:showOnCreate="{Boolean}true"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Alias"
                                                            name="./sling:alias"
                                                            renderReadOnly="{Boolean}true">
                                                            <granite:data
                                                                jcr:primaryType="nt:unstructured"
                                                                cq-msm-lockable="sling:alias"/>
                                                        </alias>
                                                    </items>
                                                </section1>
                                            </items>
                                        </column>
                                    </items>
                                </cues>
                            </items>
                        </tabs>
                    </items>
                </form>
                <contentpath
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="dam/gui/coral/components/commons/ui/shell/contentpath"
                    path="${requestPathInfo.suffix}"/>
            </editors>
        </items>
    </content>
</jcr:root>
