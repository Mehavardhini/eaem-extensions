<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <renditions jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <unifiedrenditionsouter jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <unifiedrenditionsscroll jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <unifiedrenditions jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <renditionscontainer jcr:primaryType="nt:unstructured">
                                        <items jcr:primaryType="nt:unstructured">
                                            <dropdown jcr:primaryType="nt:unstructured">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <usdz
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/option"
                                                        text="USDZ (3D)"
                                                        value="USDZ (3D)"/>
                                                </items>
                                            </dropdown>
                                        </items>
                                    </renditionscontainer>
                                    <colorpalettedata
                                        granite:class="colorpalettedata"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <usdzrenditions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/apps/eaem-usdz-support/usdz-renditions"
                                                class="usdz-renditions feature"/>
                                        </items>
                                    </colorpalettedata>
                                </items>
                            </unifiedrenditions>
                        </items>
                    </unifiedrenditionsscroll>
                </items>
            </unifiedrenditionsouter>
        </items>
    </renditions>
</jcr:root>
